{
  "expo": {
    "name": "MPLS Coffee",
    "slug": "MPLSCoffee",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#1e3237"
    },
    "plugins": [
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "Allow access to your location to find the closest coffee shop and provide accurate directions."
        }
      ]
    ],
    "ios": {
      "buildNumber": "4",
      "supportsTablet": true,
      "infoPlist": {
        "LSApplicationQueriesSchemes": ["maps"],
        "NSLocationWhenInUseUsageDescription": "Allow access to your location to find the closest coffee shop and provide accurate directions.",
        "NSLocationAlwaysUsageDescription": "Allow access to your location to find the closest coffee shop and provide accurate directions."
      },
      "bundleIdentifier": "com.parkasoftware.mplscoffee"
    },
    "android": {
      "versionCode": 2,
      "permissions": [
        "ACCESS_FINE_LOCATION",
        "android.permission.ACCESS_COARSE_LOCATION",
        "android.permission.ACCESS_FINE_LOCATION"
      ],
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#1e3237"
      },
      "package": "com.parkasoftware.mplscoffee"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "1e421fe7-060a-484a-89e5-bcf749dd8355"
      }
    },
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/1e421fe7-060a-484a-89e5-bcf749dd8355"
    }
  }
}
